<?xml version="1.0" encoding="UTF-8" ?>
<!-- /UI[UI,1]/widget[QWidget,co_i_balancepyg]/
        property[geometry]/rect[rect,1]/height[height,1]/text()[1]  -->
<patch-style name="legacy1" xmlns:xi="http://www.w3.org/2001/XInclude">
    <options>
        <value name="search-moved-blocks">never</value>
    </options>
    <entities>
        <format name="id" text="{tag}[{namelist}]">
            <xpath name="tag">name()</xpath>
            <implode name="namelist" join=",">
                <xpath>context-information/@class</xpath>
                <xpath>context-information/@name</xpath>
                <xpath>context-information/@other1</xpath>
                <xpath>context-information/@other2</xpath>
            </implode>
        </format>
        <xpath name="is-root">false()</xpath>
    </entities>
    <pre-save-patch>
        <!-- como transformar el patch a guardar -->
        <xi:include href="legacy1-savepatch1.xsl"/>
    </pre-save-patch>
</patch-style>


